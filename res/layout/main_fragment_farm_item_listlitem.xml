<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="65dp"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="10dp"
        android:gravity="center_vertical"
        android:orientation="horizontal" >

        <TextView
            android:id="@+id/farmingItem_listlitem_collectname"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="未设置"
            android:textColor="@color/text_black"
            android:textSize="15sp" />

        <ImageView
            android:id="@+id/electrialiv"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:background="@drawable/electrial_cell" />
        
        <ImageView
            android:id="@+id/xinhao_empty"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="15dp"
            android:visibility="invisible"
            android:background="@drawable/xinhao_empty" />
    </LinearLayout>

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content" >

        <LinearLayout
            android:id="@+id/collectlist_item_ll"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="4dp"
            android:paddingTop="3dp"
            android:orientation="horizontal" >

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:gravity="center" >

                <LinearLayout
                    android:id="@+id/farmingItem_listlitem_tem_ll"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal" >

                    <TextView
                        android:id="@+id/farmingItem_listlitem_tem"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="无"
                        android:textColor="@color/text_blue"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/tem_type"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/farming_from_to_temunit"
                        android:textColor="@color/text_grey"
                        android:textSize="@dimen/textunit" />
                </LinearLayout>

                <TextView
                    android:id="@+id/tem_soil"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/farmingItem_listlitem_tem_ll"
                    android:gravity="center"
                    android:text="@string/tem"
                    android:textColor="@color/text_grey"
                    android:textSize="13sp" />
            </RelativeLayout>

            <View
                android:layout_width="@dimen/listitemline_l"
                android:layout_height="match_parent"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="3dp"
                android:background="@color/list_line" />

            <RelativeLayout
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:gravity="center" >

                <LinearLayout
                    android:id="@+id/farmingItem_listlitem_hum_ll"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal" >

                    <TextView
                        android:id="@+id/farmingItem_listlitem_hum"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="无"
                        android:textColor="@color/text_blue"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/hum_type"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/farming_from_to_humunit"
                        android:textColor="@color/text_grey"
                        android:textSize="@dimen/textunit" />
                </LinearLayout>

                <TextView
                    android:id="@+id/hum_soil"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/farmingItem_listlitem_hum_ll"
                   
                    android:gravity="center"
                    android:text="@string/hum"
                    android:textColor="@color/text_grey"
                    android:textSize="13sp" />
            </RelativeLayout>

            <View
                android:id="@+id/farmingItem_listlitem_light_sliderview"
                android:layout_width="@dimen/listitemline_l"
                android:layout_height="match_parent"
                android:layout_marginTop="4dp"
                android:layout_marginBottom="3dp"
                android:background="@color/list_line" />

            <RelativeLayout
                android:id="@+id/farmingItem_listlitem_light_rl"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="3"
                android:gravity="center" >

                <LinearLayout
                    android:id="@+id/farmingItem_listlitem_light_ll"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center_horizontal" >

                    <TextView
                        android:id="@+id/farmingItem_listlitem_light"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="无"
                        android:textColor="@color/text_blue"
                        android:textSize="14sp" />

                    <TextView
                        android:id="@+id/light_type"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/farming_from_to_lightunit"
                        android:textColor="@color/text_grey"
                        android:textSize="@dimen/textunit" />
                </LinearLayout>

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/farmingItem_listlitem_light_ll"
                    android:gravity="center"
                    android:text="@string/light"
                    android:textColor="@color/text_grey"
                    android:textSize="13sp" />
            </RelativeLayout>
        </LinearLayout>
    </FrameLayout>

</LinearLayout>